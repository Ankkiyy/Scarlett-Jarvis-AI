<?xml version="1.0" encoding="ISO-8859-1"?>
<aiml version="1.0">

<category>
    <pattern>SET TALK SPEED *</pattern>
    <template>
        TALK SPEED <star />
    </template>
</category>
<category>
    <pattern>CHANGE THE TALK SPEED TO *</pattern>
    <template>
        <think><set name="TALK_SPEED"><star/></set></think>
